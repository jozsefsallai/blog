{
  "name": "sallai-blog",
  "version": "3.0.0",
  "private": true,
  "hexo": {
    "version": "3.9.0"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "hexo": "^3.8.0",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.1",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.1",
    "hexo-renderer-marked": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.3",
    "hexo-server": "^0.3.3"
  },
  "scripts": {
    "build": "cd themes/sallai && npm run prepare",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "watch": "cd themes/sallai && npm run prepare -- --watch",
    "serve": "npm run build:prod && hexo server",
    "generate": "npm run build:prod && hexo generate",
    "generate:dry": "hexo generate"
  }
}