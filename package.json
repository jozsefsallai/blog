{
  "name": "sallai-blog",
  "version": "3.0.0",
  "private": true,
  "hexo": {
    "version": "4.2.0"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "hexo": "^4.0.0",
    "hexo-all-minifier": "^0.5.3",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^1.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-lazyload": "^1.3.6",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^2.0.0",
    "hexo-renderer-stylus": "^1.1.0",
    "hexo-server": "^1.0.0"
  },
  "scripts": {
    "build": "cd themes/sallai && npm run prepare",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "watch": "cd themes/sallai && npm run prepare -- --watch",
    "serve": "npm run build:prod && hexo server",
    "serve:dev": "hexo server",
    "generate": "npm run build:prod && hexo generate",
    "generate:dry": "hexo generate"
  }
}
